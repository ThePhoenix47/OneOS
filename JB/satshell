print("SatShell v1.0 by JBPhoenix")
print("Type 'end' to exit")
print("Using RedNet Technology")
print("Using Satellite PhoenixSat-1")
print("==========================")
rednet.open("back")
satID=rednet.lookup("PhoenixSat_LiveShell","sat1")
while true do
  msg=read()
  if msg=="end" then
    break
  end
  rednet.send(satID,msg,"PhoenixSat_LiveShell")
end
